# Environment
NODE_ENV=development

# Server
PORT=9000

# Logging
LOG_LEVEL=info
LOG_TO_CONSOLE=true
LOG_TO_FILE=true
LOG_MAX_SIZE=5242880
LOG_MAX_FILES=5

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
DOWNLOAD_LIMIT_WINDOW_MS=3600000
DOWNLOAD_LIMIT_MAX_REQUESTS=10

# Temporary Files
TEMP_DIR=./temp 